<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Exo7.Exo7">


    <TextView
            android:textSize="20sp"
            android:gravity="center"
            android:layout_width="144dp"
            android:layout_height="66dp" android:id="@+id/latitude"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.104" app:layout_constraintHorizontal_bias="0.763"/>
    <TextView
            android:textSize="20sp"
            android:gravity="center"
            android:layout_width="148dp"
            android:layout_height="59dp" android:id="@+id/longitude"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.017" app:layout_constraintHorizontal_bias="0.804"/>
    <TextView
            android:text="Latitude : "
            android:textSize="20sp"
            android:gravity="center"
            android:layout_width="117dp"
            android:layout_height="42dp" android:id="@+id/text1"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.038" app:layout_constraintHorizontal_bias="0.137"/>
    <TextView
            android:text="Longitude : "
            android:textSize="20sp"
            android:gravity="center"
            android:layout_width="117dp"
            android:layout_height="42dp" android:id="@+id/text2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.117" app:layout_constraintHorizontal_bias="0.137"/>
    <androidx.fragment.app.FragmentContainerView android:layout_width="346dp" android:layout_height="521dp"
                                                 android:id="@+id/maps" app:layout_constraintEnd_toEndOf="parent"
                                                 app:layout_constraintStart_toStartOf="parent"
                                                 app:layout_constraintBottom_toBottomOf="parent"
                                                 app:layout_constraintTop_toTopOf="parent"
                                                 app:layout_constraintVertical_bias="0.912"
                                                 android:name="com.google.android.gms.maps.SupportMapFragment"
                                                 app:layout_constraintHorizontal_bias="0.489">


    </androidx.fragment.app.FragmentContainerView>


</androidx.constraintlayout.widget.ConstraintLayout>